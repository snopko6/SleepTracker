<resources>
    <!-- Base application theme. -->
    <style name="Theme.SleepTracker" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/farbaPrimarna</item>
        <item name="colorPrimaryVariant">@color/farbaPrimarnaVariant</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/farbaPrimarna</item>
        <item name="colorSecondaryVariant">@color/farbaText</item>
        <item name="colorOnSecondary">@color/farbaPozadie</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/statusBar</item>
        <item name="android:windowBackground">@color/farbaPozadie</item>
        <!-- Customize your theme here. -->
    </style>

    <color name="endColor">#E4ECF1</color>
</resources>